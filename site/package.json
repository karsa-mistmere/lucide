    "eslint": "^8.38.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "jest": "^29.5.0",
    "node-fetch": "^2.6.9",
    "prettier": "^2.8.7",
    "react-test-renderer": "18.2.0",
    "ts-node": "~10.9.1",
    "tslib": "^2.5.0",
    "typescript": "^5.0.4"
