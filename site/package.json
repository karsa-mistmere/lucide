    "@emotion/styled": "^11.10.6",
    "@mdx-js/loader": "^2.3.0",
    "@mdx-js/react": "^2.3.0",
    "@next/mdx": "^13.3.0",
    "@svgr/webpack": "^7.0.0",
    "@types/node-fetch": "^2.6.3",
    "dotenv": "^16.0.3",
    "downloadjs": "^1.4.7",
    "element-to-path": "^1.2.1",
    "framer-motion": "^10.11.2",
    "fuse.js": "^6.6.2",
    "gray-matter": "^4.0.3",
