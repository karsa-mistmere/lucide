{
  "name": "lucide-angular",
  "description": "A Lucide icon library package for Angular applications",
  "version": "0.143.0",
  "author": "SMAH1",
  "license": "ISC",
  "homepage": "https://lucide.dev",
  "bugs": "https://github.com/lucide-icons/lucide/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/lucide-icons/lucide.git",
    "directory": "packages/lucide-angular"
  },
  "publishConfig": {
    "directory": "dist"
  },
  "keywords": [
    "Lucide",
    "Angular",
    "Feather",
    "Icons",
    "Icon",
    "Feather Icons",
    "Fontawesome",
    "Font Awesome"
  ],
  "scripts": {
    "build": "pnpm clean && pnpm copy:license && pnpm build:icons && pnpm build:ng",
    "copy:license": "cp ../../LICENSE ./LICENSE",
    "clean": "rm -rf dist && rm -rf ./src/icons/*.ts",
    "build:icons": "build-icons --output=./src --templateSrc=./scripts/exportTemplate.mjs --iconFileExtension=.ts --exportFileName=lucide-icons.ts",
    "build:ng": "ng build --configuration production",
    "test": "ng test --no-watch --no-progress --browsers=ChromeHeadlessCI",
    "test:watch": "ng test",
    "lint": "npx eslint 'src/**/*.{js,jsx,ts,tsx,html,css,scss}' --quiet --fix",
    "format": "npx prettier 'src/**/*.{js,jsx,ts,tsx,html,css,scss}' --write",
    "e2e": "ng e2e",
    "version": "pnpm version --git-tag-version=false"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "13.x",
    "@angular-eslint/builder": "13.x",
    "@angular-eslint/eslint-plugin": "13.x",
    "@angular-eslint/eslint-plugin-template": "13.x",
    "@angular-eslint/schematics": "13.x",
    "@angular-eslint/template-parser": "13.x",
    "@angular/cli": "13.x",
    "@angular/common": "13.x",
    "@angular/compiler": "13.x",
    "@angular/compiler-cli": "13.x",
    "@angular/core": "13.x",
    "@angular/platform-browser": "13.x",
    "@angular/platform-browser-dynamic": "13.x",
    "@lucide/build-icons": "workspace:*",
    "@types/jasmine": "~4.3.0",
    "@types/node": "^18.11.4",
    "@typescript-eslint/eslint-plugin": "5.48.2",
    "@typescript-eslint/parser": "5.48.2",
    "eslint": "^8.33.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jasmine-core": "~4.5.0",
    "jasmine-spec-reporter": "~7.0.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.0.0",
    "ng-packagr": "13.x",
    "prettier": "^2.8.4",
    "protractor": "~7.0.0",
    "puppeteer": "^19.8.5",
    "rxjs": "7.5.7",
    "ts-node": "~10.9.1",
    "tslib": "^2.4.0",
    "typescript": "4.4.2 - 4.5.0",
    "zone.js": "~0.12.0"
  },
  "peerDependencies": {
    "@angular/core": "13.x - 15.x"
  }
}
